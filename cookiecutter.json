{
    "project_name": "Pyramid Scaffold",
    "repo_name": "{{cookiecutter.project_name.lower().strip().replace(' ', '_').replace(':', '_').replace('-', '_').replace('!', '_')}}",
    "template_language": ["jinja2", "chameleon", "mako"],
    "persistence": ["none", "sqlalchemy-postgres", "sqlalchemy-sqlite", "zodb"],
    "rest": ["none", "cornice"],
    "schemas": ["none", "marshmallow"],
    "authentication": ["none", "jwt"],
    "rpc": ["none", "grpc"],
    "tasks": ["none", "celery"],
    "orchestrator": ["none", "conductor"],
    "services": ["none", "pyramid-services"],
    "docs": ["none", "sphinx"],
    "_copy_without_render": [
        "static/*"
    ]
}
